{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "./dist",
    "types": ["node"],
    "composite": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "include": [".", "./worker-configuration.d.ts"]
}
